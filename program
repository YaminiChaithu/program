
#include <stdio.h>

int main()
{
    int i,n,j,mul;
    printf("n value:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        mul=1;
        for(j=1;j<=n;j++)
        {
            mul=mul*j*i;
            printf("%d\t",mul);
            mul=1;
        }
        printf("\n");
    }
    return 0;
}
